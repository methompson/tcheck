{
  "name": "tcheck",
  "version": "1.2.3",
  "description": "A TypeCheck / TypeGuard Library",
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": {
        "import": "./dist/index.d.ts",
        "require": "./dist/index.d.cts"
      },
      "node": {
        "import": "./dist/index.js",
        "require": "./dist/index.cjs"
      },
      "import": "./dist/index.js",
      "require": "./dist/idnex.cjs"
    },
    "./package.json": "./package.json"
  },
  "sideEffects": false,
  "scripts": {
    "build": "tsup",
    "lint": "eslint",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/methompson/tcheck.git"
  },
  "homepage": "https://github.com/methompson/tcheck",
  "author": "Mathew E. Thompson",
  "license": "Apache-2.0",
  "devDependencies": {
    "@jest/globals": "^29.7.0",
    "@types/jest": "^29.5.14",
    "@typescript-eslint/parser": "^8.19.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "jest": "^29.7.0",
    "prettier": "^3.4.2",
    "ts-jest": "^29.2.5",
    "tsup": "^8.3.0",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.19.1"
  }
}